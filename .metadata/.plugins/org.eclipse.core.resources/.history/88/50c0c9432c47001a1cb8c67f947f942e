package arraysMulti;

//An array is a container which stores multiple values of same data type.. 

public class M1 {
	public static void main(String[] args) {
		
		int a;
		a=4;
		
		int b[] = new int[5]; // Declaration of an array and allocates memory for the value.
		int c[] = new int[10];  // Declaration of an array and allocates memory for the value.
		
		a[0]= 2;
		a[1]= 6;
		a[2]= 1;
		a[3]= 9;
		a[4]= 12;
		
	}

}