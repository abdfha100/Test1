package formatters04;

public class M5 {
public static void main(String[] args) {
		
		int i = 100;
		double j = 2.5;
		boolean k = true;
		String s1 = "abc";
		char c1 = 't';
		
		System.out.printf("i value is %d", i);	
		System.out.printf("\n j value is %f", j);
		System.out.printf("\n k value is %b", k);
		System.out.printf("\n s1 value is %s", s1);
		System.out.printf("\n c1 value is %c", c1);
	}

}
