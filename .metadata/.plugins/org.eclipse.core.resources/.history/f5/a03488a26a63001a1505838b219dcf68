package com.lara;

public class M50 {
public static void main(String[] args) {
		
		String s1 = "java technologies";
		//           0123456789      16
		String s2 = s1.substring(5, 18);
		String s3 = s1.substring(0, 4);
		
		System.out.println(s1);
		System.out.println(s2);
		System.out.println(s3);
	}

}
/*
java technologies
technologies
java

